{
  "manifest_version": 2,
  "name": "UpgradeMixedContentByBlacklist",
  "version": "0.3.3",
  "description": "Resloves some mixedcontent blocking.",
  "icons": {
    "48": "baseline_enhanced_encryption_black_48dp.png",
    "96": "baseline_enhanced_encryption_black_96dp.png"
  },
  "permissions": ["https://*/*", "webRequest", "webRequestBlocking"],
  "background": {
    "scripts": ["background.js", "rule.js"]
  },
  "options_ui": {
    "page": "options/options.html",
    "browser_style": true,
    "open_in_tab": true
  }
}
